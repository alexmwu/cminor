// Test case 4
// Global declarations

a: integer = 15;
a_: integer;

b: boolean = true;
b_: boolean;

c: char = 'c';
c_: char;

s: string = "abcde";
s_: string;

main: function integer () = {

  print "expected:\n";
  print "15 true c abcde\n";
  print "actual:\n";
  print a, ' ', b, ' ', c, ' ', s, '\n';
  a = a_;
  a_ = a;
  b_ = b;
  c_ = c;
  s_ = s;
  print "expected:\n";
  print "15 true c abcde\n";
  print "actual:\n";
  print a_, ' ', b_, ' ', c_, ' ', s_, '\n';

  return 0;
}
